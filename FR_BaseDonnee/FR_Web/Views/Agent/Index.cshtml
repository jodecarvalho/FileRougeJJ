@model IEnumerable<FR_Web.Models.Agent>
@{
    ViewBag.Title = "Index";
}

<h2>Index Agent</h2>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AgentId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AgentId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.AgentId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.AgentId })
            </td>
        </tr>
    }
</table>

