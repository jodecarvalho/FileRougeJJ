@model IEnumerable<FR_Web.Models.QuestionReponse>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/QuestionReponse/_QrLayout.cshtml";
}

<h2>Index QuestionReponse</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.QuestionId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReponseId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Vraie)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.QuestionId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReponseId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Vraie)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { QuestionId = item.QuestionId, ReponseId = item.ReponseId }) 
        </td>
    </tr>
}

</table>
